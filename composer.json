{
    "name": "prestaflow/php-library",
    "description": "PrestaFlow is an open-source set of prebuilt tests components, ready-to-use examples made for the PrestaShop E-commerce software.",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "PrestaFlow\\Library\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Jonathan Danse",
            "email": "j.danse@prestaedit.com"
        }
    ],
    "scripts": {
        "tests": "./bin/prestaflow run src/Tests"
    },
    "require": {
        "nunzion/php-expect": "^1.1",
        "chrome-php/chrome": "^1.10",
        "symfony/console": "^5.4",
        "vlucas/phpdotenv": "^5.6",
        "symfony/event-dispatcher": "^5.4"
    },
    "config": {
        "platform": {
            "php": "7.4.15"
        }
    }
}
